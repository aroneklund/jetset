        title = "Jetset: selecting the optimal  microarray probe set to represent a gene",
        author = personList(as.person("Qiyuan Li"),
                            as.person("Nicolai J Birkbak"),
                            as.person("Balazs Gyorffy"),
                            as.person("Zoltan Szallasi"),
                            as.person("Aron C Eklund")),
        year = 2011,
        journal = 'BMC Bioinformatics',
        volume = 12,
        pages = 474
)
